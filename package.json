{
  "private": true,
  "name": "dev-site-documentation-template",
  "version": "1.0.0",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/AdobeDocs/dev-site-documentation-template"
  },
  "author": {
    "name": "Bruce Denham",
    "url": "https://github.com/bdenham"
  },
  "browser": {
    "tty": "tty-browserify"
  },
  "dependencies": {
    "@adobe/gatsby-theme-aio": "^3.21.0",
    "@emotion/react": "^11.4.1",
    "@spectrum-css/button": "^4.0.0",
    "@spectrum-css/divider": "^1.0.4",
    "@spectrum-css/icon": "^3.0.4",
    "@spectrum-css/link": "^3.1.4",
    "@spectrum-css/page": "^3.0.3",
    "@spectrum-css/typography": "^4.0.0",
    "@spectrum-css/vars": "^4.0.0",
    "classnames": "^2.3.1",
    "gatsby": "^3.6.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "tty-browserify": "^0.0.1"
  },
  "scripts": {
    "start": "gatsby build && gatsby serve",
    "start:prefix": "gatsby build --prefix-paths && gatsby serve --prefix-paths",
    "dev": "gatsby develop",
    "dev:https": "gatsby develop --https --host localhost.corp.adobe.com --port 9000",
    "build": "gatsby build",
    "build:incremental": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint": "npm run lint:md src/pages",
    "lint:md": "remark --quiet",
    "prettier": "prettier --write",
    "prettier:test": "prettier --check src"
  },
  "devDependencies": {
    "prettier": "2.4.1",
    "remark-cli": "^10.0.0",
    "remark-lint-list-item-indent": "^3.1.0",
    "remark-preset-lint-recommended": "^6.1.1"
  }
}
